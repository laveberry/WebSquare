    				</xf:group>
    				<xf:group tagname="th" style="" class="w2tb_th">성별<w2:attributes>
    						<w2:scope>row</w2:scope>
    					</w2:attributes>
    				</xf:group>
    				<xf:group tagname="td" style="" class="w2tb_td">
    					<xf:select1 id="" selectedIndex="0" appearance="full" style="min-width: 150px;min-height: 21px;" ref="" rows="1">

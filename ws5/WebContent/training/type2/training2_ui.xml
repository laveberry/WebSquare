    						<w2:scope>row</w2:scope>
    					</w2:attributes>
    				</xf:group>
    				<xf:group tagname="td" style="" class="w2tb_td">
    					<w2:inputCalendar footerDiv="false" id="" weekStartsOn="0" style="width: 150px;height: 23px;" renderDiv="true"
    						focusOnDateSelect="false" calendarValueType="yearMonthDate" rightAlign="false" renderType="component" ref="">
    					</w2:inputCalendar>
    				</xf:group>
    				<xf:group tagname="th" style="" class="w2tb_th">직급<w2:attributes>
    						<w2:scope>row</w2:scope>
    					</w2:attributes>
    				</xf:group>
    				<xf:group tagname="td" style="" class="w2tb_td">
    					<xf:select1 allOption="false" appearance="minimal" chooseOption="" direction="auto" disabled="false"
